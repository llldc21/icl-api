{
  "name": "icl-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "yarn lint --fix --quiet",
    "start": "ts-node --transpile-only src/shared/infra/http/server.ts",
    "build": "yarn clean && yarn compile",
    "test": "jest --passWithNoTests",
    "test:staged": "jest --passWithNoTests --silent --noStackTrace --findRelatedTests --collectCoverage=false",
    "test:dev": "jest --watchAll --passWithNoTests",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "date-fns-tz": "^1.0.12",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "husky": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.4.2",
    "morgan": "^1.10.0",
    "pg": "^8.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.1.4",
    "tslog": "^2.9.1",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.28",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "git-commit-msg-linter": "^3.0.0",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
        "pre-commit": "lint-staged",
        "pre-push": "yarn test"
    }
}
}
